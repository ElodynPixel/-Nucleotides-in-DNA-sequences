        "#counting nucleotide frequency of a given DNA sample\n",
        "DNA = input (\"please enter your DNA sequence below:\\n\")\n",
        "DNA = DNA.upper()\n",
        "A = DNA.count(\"A\")\n",
        "T = DNA.count(\"T\")\n",
        "C = DNA.count(\"C\")\n",
        "G = DNA.count(\"G\")\n",
        "\n",
        "print(\"the number of A in your DNA sequence is: \",A)\n",
